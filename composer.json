{
    "name": "mindtwo/laravel-multilingual",
    "description": "Laravel Multilingual",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://www.mindtwo.de/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "laravel/framework": "6.*|^7.0|^8.0",
        "mindtwo/laravel-dynamic-model-mutators": "^3.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "mindtwo\\LaravelMultilingual\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mindtwo\\LaravelMultilingual\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\LaravelMultilingual\\Providers\\MultilingualServiceProvider",
                "mindtwo\\LaravelMultilingual\\Providers\\EventServiceProvider",
                "mindtwo\\LaravelMultilingual\\Providers\\TranslationServiceProvider"
            ]
        }
    }
}
